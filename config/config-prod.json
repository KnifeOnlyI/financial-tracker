{
  "port": 80,
  "database": {
    "host": "financial-tracker-postgresql",
    "port": 5432,
    "name": "%FINANCIAL_TRACKER_DATABASE_NAME%",
    "user": "%FINANCIAL_TRACKER_DATABASE_USER%",
    "password": "%FINANCIAL_TRACKER_DATABASE_PASSWORD%",
    "logs": false
  },
  "emails": {
    "sender": {
      "from": "%FINANCIAL_TRACKER_SENDER_EMAIL_USER%",
      "host": "%FINANCIAL_TRACKER_SENDER_EMAIL_HOST%",
      "port": "%FINANCIAL_TRACKER_SENDER_EMAIL_PORT%",
      "username": "%FINANCIAL_TRACKER_SENDER_EMAIL_USER%",
      "password": "%FINANCIAL_TRACKER_SENDER_EMAIL_PASSWORD%",
      "ignoreTLS": true,
      "protocol": "smtp",
      "auth": true
    }
  },
  "view": {
    "templates": "views"
  }
}
